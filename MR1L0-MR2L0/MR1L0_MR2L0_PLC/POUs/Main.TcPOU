               nYdwnEncRef:=GVL_M1L0.nYDWN_ENC_REF,
               nXupEncRef:=GVL_M1L0.nXUP_ENC_REF,
               nXdwnEncRef:=GVL_M1L0.nXDWN_ENC_REF,
