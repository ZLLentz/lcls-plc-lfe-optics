	M1L0 : DUT_HOMS;
	
	// M2L0
	M7 : DUT_MotionStage:=(nCommand:=ENUM_EpicsMotorCmd.EPICS_MOTOR_MOVE_ABS); // Yup
	{attribute 'pytmc' := '
		pv: M2L0_YUP
	'}
	fbMotionStage_m7 : FB_MotionStage;

	M8 : DUT_MotionStage:=(nCommand:=ENUM_EpicsMotorCmd.EPICS_MOTOR_MOVE_ABS); // Ydwn
	{attribute 'pytmc' := '
		pv: M2L0_YDWN
	'}
